# JWT settings
JWT_SECRET_KEY=define_jwt_secre_key
JWT_EXPIRATION=define_jwt_expires_in_in_seconds
JWT_REFRESH_EXPIRATION=define_jwt_refresh_expires_in_seconds

# Crypt settings
CRYPT_SALT=define_bcrypt_salt